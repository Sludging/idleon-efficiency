    {
      "label": "points_owned",
      "expression": "idleon.callFunction(\"Thingies\", \"LegendPTS_owned\", 0, 0)",
      "description": "Total legend talent points owned from all sources"
    },
    {
      "label": "points_spent",
      "expression": "idleon.callFunction(\"Thingies\", \"LegendPTS_spent\", 0, 0)",
      "description": "Total points spent (sum of all talent levels)"
    },
