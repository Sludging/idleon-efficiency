    },
    {
      "label": "sailing_artifact_34_raw",
      "expression": "idleon.getAttr(\"Sailing\")[3][34]",
      "description": "Sailing artifact 34 raw bonus (before min/round)"
    },
    {
      "label": "sailing_artifact_34_processed",
